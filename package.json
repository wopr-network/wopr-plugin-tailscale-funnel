{
  "author": "WOPR",
  "description": "Expose WOPR services externally via Tailscale Funnel",
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/node": "^25.3.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "wopr",
    "plugin",
    "tailscale",
    "funnel",
    "tunnel"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "name": "@wopr-network/wopr-plugin-tailscale-funnel",
  "scripts": {
    "build": "tsc",
    "check": "biome check --config-path=. src/ && tsc --noEmit",
    "dev": "tsc --watch",
    "format": "biome format --config-path=. --write src/",
    "lint": "biome check --config-path=. src/",
    "lint:fix": "biome check --config-path=. --fix src/",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "version": "1.0.0",
  "dependencies": {
    "@wopr-network/plugin-types": "^0.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "release": {
    "extends": "@wopr-network/semantic-release-config"
  }
}
